{
  "name": "fastify-paginate",
  "version": "0.1.0",
  "description": "pagination plugin for fastify",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest -c jest.config.js --coverage",
    "format:write": "prettier --write \"src/**/*.ts\" \"__test__/**/*.ts\"",
    "format": "prettier --check \"src/**/*.ts\" \"__test__/**/*.ts\"",
    "clean": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "license-checker": "license-checker --production --onlyAllow=\"MIT;ISC;BSD-3-Clause;BSD-2-Clause\"",
    "lint": "eslint \"src/**/*.ts\" \"__test__/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\" \"__test__/**/*.ts\""
  },
  "keywords": [
    "fastify",
    "pagination",
    "paginate",
    "REST"
  ],
  "author": "Matthias Keckl",
  "license": "MIT",
  "dependencies": {
    "fastify-plugin": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "fastify": "^4.15.0",
    "jest": "^29.5.0",
    "license-checker": "^25.0.1",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0"
  }
}
